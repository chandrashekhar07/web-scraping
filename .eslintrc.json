{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": ["./tsconfig.json"],
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-magic-numbers": 0,
    "no-console": 2,
    "no-eval": 2,
    "no-invalid-this": 2,
    "no-return-await": 2,
    "no-unsafe-finally": 2,
    "prefer-const": 2,
    "arrow-parens": 0,
    "no-empty": 2,
    "prefer-template": 2,
    "no-prototype-builtins": 0,
    "@typescript-eslint/no-var-requires": 1,
    "no-useless-catch": 1,
    "@typescript-eslint/no-inferral-type": 0,
    "prettier/prettier": 2,
    "no-dupe-args": "error",
    "no-dupe-keys": "error",
    "no-extra-parens": "off",
    "no-extra-semi": "error",
    "no-func-assign": "error",
    "no-invalid-regexp": "error",
    "block-scoped-var": "off",
    "curly": "error",
    "default-case": "off",
    "eqeqeq": "error",
    "no-empty-pattern": "error",
    "no-eq-null": "error",
    "no-implicit-globals": "off",
    "no-multi-spaces": "off",
    "no-redeclare": "error",
    "no-return-assign": "off",
    "no-self-compare": "off",
    "no-self-assign": "error",
    "no-unmodified-loop-condition": "off",
    "no-unused-expressions": "off",
    "no-void": "off",
    "vars-on-top": "off",
    "no-undef-init": "off",
    "no-use-before-define": "off",
    "array-bracket-spacing": "off",
    "block-spacing": "off",
    "brace-style": "off",
    "camelcase": "off",
    "eol-last": "off",
    "key-spacing": "off",
    "keyword-spacing": "off",
    "new-parens": "off",
    "newline-after-var": "off",
    "newline-before-return": "off",
    "no-continue": "off",
    "no-mixed-spaces-and-tabs": "error",
    "no-multiple-empty-lines": "off",
    "no-ternary": "off",
    "no-unneeded-ternary": "off",
    "no-whitespace-before-property": "off",
    "object-curly-newline": "off",
    "object-curly-spacing": "off",
    "object-property-newline": "off",
    "semi-spacing": "off",
    "space-before-function-paren": "off",
    "space-in-parens": "off",
    "space-infix-ops": "off",
    "space-unary-ops": "off",
    "spaced-comment": "off",
    "no-const-assign": "error",
    "no-dupe-class-members": "error",
    "no-duplicate-imports": "error",
    "no-useless-constructor": "off",
    "no-var": "off"
  }
}
